{
  "name": "deep-search",
  "private": true,
  "version": "0.1.1",
  "description": "Supercharge your browsers find with regex and deep search",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp --production --vendor=chrome && gulp pack --vendor=chrome",
    "build:firefox": "gulp --production --vendor=firefox && gulp pack --vendor=firefox",
    "build:opera": "gulp --production --vendor=opera && gulp pack --vendor=opera",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "test": "mocha --require babel-core/register --recursive test --watch"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.16.3",
    "chai": "^3.2.0",
    "debounce": "^1.0.0",
    "del": "^2.0.2",
    "eslint": "3.12.2",
    "eslint-loader": "1.6.1",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.0.1",
    "gulp-cache": "^0.4.3",
    "gulp-filter": "^4.0.0",
    "gulp-git": "^1.2.4",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-json-transform": "^0.3.0",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-sequence": "^0.4.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.5",
    "gulp-zip": "^3.0.2",
    "json-loader": "0.5.4",
    "mocha": "^2.5.3",
    "require-dir": "^0.3.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.12.14",
    "webpack-stream": "^3.1.0",
    "yargs": "^4.3.2"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "findandreplacedomtext": "^0.4.4",
    "jquery": "^2.2.4",
    "json2csv": "^3.7.0",
    "lodash.debounce": "4.0.8",
    "queue": "^4.0.0",
    "tooltipster": "^4.1.5"
  }
}
